        <footer>
			<OverflowToolbar>
				<!-- <ToolbarSpacer/>
				<Button type="Accept" text="Save"/>
				<Button type="Reject" text="Cancel"/> -->
                <ToolbarSpacer/>
                <Button type="Accept" text="Корзина" press=".onBasket"/>
